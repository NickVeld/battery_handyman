[run]
branch = True
concurrency = multiprocessing
source = battery_handyman

[report]
exclude_lines =
    if self.debug:
    pragma: no cover
    raise NotImplementedError
    if __name__ == .__main__.:
    if testing:
ignore_errors = True
omit =
    tests/*
    */__main__.py
    bin/*
    setup.py

[xml]
output = coverage.xml
